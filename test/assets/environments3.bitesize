project: test
environments:
- name: environment1
  namespace: environment-dev
  imports_repository: "git@bitbucket.pearson.com/configmap.git"
  imports:
     - name: "application"
       files:
          - "k8s/application-v1.bitesize/application-v2.properties"
       version: 1.0.1
       type: configmap
     - name: "application-v1"
       files:
          - "k8s/application-v1.bitesize/application-v2.properties"
       version: 1.0.0
       type: configmap
     - name: "application-v2"
       files:
          - "k8s/application-v1.bitesize/application-v2.properties"
       type: configmap
  services:
  - name: service1_environment1
    version: 1.0.0
    volumes:
      - name: application-v1
        path: "/etc/config/application-v1.config"
        type: configmap